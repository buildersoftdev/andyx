{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss}|[{Level}]|{Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "Credentials": {
    "Username": "admin",
    "Password": "admin"
  },

  "AllowedHosts": "*"
}
